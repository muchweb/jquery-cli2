{
  "name": "jquery-cli2",
  "description": "Query HTML",
  "version": "0.1.1",
  "repository": "https://github.com/muchweb/jquery-cli2",
  "license": "GPL-3.0+",
  "main": "lib/main.js",
  "author": "muchweb <aleks@s-ko.net>",
  "devDependencies": {
    "coffee-script": "^1.8.0",
    "nodeunit": "^0.9.0"
  },
  "bin": {
    "jquery-cli2": "lib/cli"
  },
  "scripts": {
    "compile": "./node_modules/.bin/coffee --map --compile --output lib src",
