    "prepublish": "npm run compile",
    "pretest": "npm run compile",
    "test": "./node_modules/.bin/nodeunit"
  },
  "dependencies": {
    "commander": "^2.5.0",
    "jquery": "^2.1.1",
    "jsdom": "^1.2.2"
  }
